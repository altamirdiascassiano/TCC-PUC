    "classnames": "^2.2.6",
    "date-fns": "^2.16.1",
    "docz": "^2.3.1",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "react-dropzone": "^11.2.4",
    "react-input-mask": "^2.0.4",
