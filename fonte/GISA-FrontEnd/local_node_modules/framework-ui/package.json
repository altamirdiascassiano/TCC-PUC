{
  "name": "framework-ui",
  "version": "1.2.6",
  "description": "Framework UI React Library based on Material UI",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
